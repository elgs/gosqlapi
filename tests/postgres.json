{
  "web": {
    "http_addr": "127.0.0.1:8080",
    "cors": true,
    "http_headers": {
      "abc": "123"
    }
  },
  "databases": {
    "test_db": {
      "type": "postgres",
      "url": "env:postgres_url"
    }
  },
  "scripts": {
    "init": {
      "database": "test_db",
      "path": "tests/init.sql",
      "public_exec": true
    },
    "metadata": {
      "database": "test_db",
      "path": "tests/metadata.sql"
    },
    "tables": {
      "database": "test_db",
      "sql": "SELECT table_name as NAME FROM information_schema.tables WHERE table_schema = 'public' ORDER BY table_name",
      "public_exec": true
    }
  },
  "tables": {
    "test_table": {
      "database": "test_db",
      "name": "TEST_TABLE",
      "public_read": true,
      "public_write": true
    },
    "token_table": {
      "database": "test_db",
      "name": "TOKENS"
    }
  },
  "managed_tokens": {
    "database": "test_db",
    "query_path": "tests/token_query.sql"
  },
  "cache_tokens": true
}