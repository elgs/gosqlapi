{
  "web": {
    "http_addr": "127.0.0.1:8080",
    "cors": true,
    "http_headers": {
      "abc": "123"
    }
  },
  "databases": {
    "test_db": {
      "type": "sqlite3",
      "url": ":memory:"
    }
  },
  "scripts": {
    "init": {
      "database": "test_db",
      "path": "scripts/init.sql",
      "public_exec": true
    },
    "metadata": {
      "database": "test_db",
      "path": "scripts/metadata.sql"
    },
    "list_tables": {
      "database": "test_db",
      "sql": "SELECT name FROM sqlite_master WHERE type='table' and name like 'TEST_GOSQLAPI%' ORDER BY name",
      "public_exec": true
    },
    "list_columns": {
      "database": "test_db",
      "sql": "SELECT * FROM PRAGMA_TABLE_INFO(?table_name?)",
      "public_exec": true
    }
  },
  "tables": {
    "test_table": {
      "database": "test_db",
      "name": "TEST_GOSQLAPI",
      "public_read": true,
      "public_write": true
    },
    "token_table": {
      "database": "test_db",
      "name": "TEST_GOSQLAPI_TOKENS"
    }
  },
  "managed_tokens": {
    "database": "test_db",
    "query_path": "scripts/token_query.sql"
  },
  "null_value": "NULL"
}