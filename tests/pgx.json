{
  "web": {
    "http_addr": "127.0.0.1:8080",
    "cors": true,
    "http_headers": {
      "abc": "123"
    }
  },
  "databases": {
    "test_db": {
      "type": "pgx",
      "url": "env:pgx_url"
    }
  },
  "scripts": {
    "init": {
      "database": "test_db",
      "path": "scripts/init.sql",
      "public_exec": true
    },
    "metadata": {
      "database": "test_db",
      "path": "scripts/metadata.sql"
    },
    "list_tables": {
      "database": "test_db",
      "sql": "SELECT table_name as NAME FROM information_schema.tables WHERE table_schema = 'public' and table_name like 'test_gosqlapi%' ORDER BY table_name",
      "public_exec": true
    },
    "list_columns": {
      "database": "test_db",
      "path": "scripts/list_columns_postgres_sqlserver.sql",
      "public_exec": true
    }
  },
  "tables": {
    "test_table": {
      "database": "test_db",
      "name": "TEST_GOSQLAPI",
      "public_read": true,
      "public_write": true
    },
    "token_table": {
      "database": "test_db",
      "name": "TEST_GOSQLAPI_TOKENS"
    }
  },
  "managed_tokens": {
    "database": "test_db",
    "query_path": "scripts/token_query.sql"
  },
  "cache_tokens": true,
  "null_value": "NULL"
}