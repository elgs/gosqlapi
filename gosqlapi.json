{
  "web": {
    "http_addr": "127.0.0.1:8080",
    "cors": true
  },
  "databases": {
    "test_db": {
      "type": "sqlite",
      "url": ":memory:"
    }
  },
  "scripts": {
    "init": {
      "database": "test_db",
      "path": "init.sql",
      "public_exec": true
    }
  },
  "tables": {
    "test_table": {
      "database": "test_db",
      "name": "TEST_TABLE",
      "exported_columns": [
        "NAME"
      ],
      "public_read": true,
      "public_write": true
    },
    "token_table": {
      "database": "test_db",
      "name": "TOKENS",
      "show_total": true
    }
  },
  "managed_tokens": {
    "database": "test_db",
    "query_path": "token_query.sql"
  }
}